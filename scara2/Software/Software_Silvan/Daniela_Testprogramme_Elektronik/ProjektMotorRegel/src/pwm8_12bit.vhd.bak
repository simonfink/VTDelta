LIBRARY IEEE;
USE IEEE.std_logic_1164.ALL;
USE IEEE.numeric_std.ALL;

PACKAGE de2_115_start_pkg IS
	TYPE t_pwm8_12bit_value IS ARRAY (0 to 11) OF unsigned(11 downto 0);                        		
	
	
	COMPONENT pwm8_12bit IS
	PORT (	
		isl_clk : IN std_logic;
		it_values: in t_pwm8_12bit_value;
		oslv8_pwm: OUT std_logic(11 downto 0);
	) ;
	END COMPONENT de2_115_start;
END PACKAGE pwm8_pkg;